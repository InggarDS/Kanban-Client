(-50%, -50%);\r\n    margin-top: 5%;\r\n    margin-bottom: 5%;\r\n    border-radius: 4px;\r\n}\r\n.login-form, .register-form {\r\n  \r\n    margin: auto;\r\n    top: 50px;\r\n    display: flex;\r\n    flex-direction:row;\r\n    justify-content: flex-start;\r\n    width: 600px;\r\n    height: 450px;\r\n    border-radius: 2px;\r\n  \r\n    box-shadow: 5px 0px 10px 0px rgb(177, 177, 177);\r\n}\r\n\r\n\r\n\r\nh2 {\r\n    color: $bgColor;\r\n    font-size: 24px;\r\n}\r\n\r\n.left {\r\n   margin: 10px;\r\n    \r\n}\r\n\r\n.right {\r\n    background-color:$bgColor;\r\n    width: 200px;\r\n    flex: 1;\r\n    \r\n}\r\n\r\n.right h1 {\r\n    color: white;\r\n    font-family: $lobster;\r\n    text-align: center;\r\n    font-size: 64px;\r\n    margin-top: 40%;\r\n    opacity: 90%;\r\n}\r\n\r\n.left form input[type=text]:focus {\r\n    border-bottom: 1px solid $bgColor;\r\n    box-shadow: 0 1px 0 0 #000;\r\n  }\r\n\r\np {\r\n    font-size: 12px;\r\n   color: grey;\r\n}\r\n\r\n\r\n//dashboard\r\n.container {\r\n    margin: 0;\r\n    padding: 0;\r\n    height: 100vh;\r\n    width: 100vw;\r\n    display:flex;\r\n    flex-direction:column;\r\n    background-color:$bgColor;\r\n    // @include bacgroundImage('/img/bg.jpg');\r\n}\r\n\r\n.navbar {\r\n    margin: 0;\r\n    padding: 0;\r\n    display: flex;\r\n    flex-direction: row;\r\n    background-color: $navColor;\r\n    height:60px;\r\n    opacity: 32%;\r\n    justify-content: space-between;\r\n\r\n} \r\n\r\n.kanban-title h4 {\r\n    color: white;\r\n    flex: 1;\r\n    margin: 5px;\r\n    font-family: $lobster;\r\n}\r\n\r\n.navbar #logout-btn {\r\n    opacity: 100%;\r\n    color: white;\r\n    border: none;\r\n    margin: 5px;\r\n    background-color:rgb(201, 201, 201);\r\n    border-radius: 2px;\r\n    width: 64px; \r\n   \r\n}\r\n\r\n.navbar #logout-btn:hover {\r\n    cursor: pointer; \r\n    // opacity: 50%;\r\n}\r\n\r\n\r\n.content {\r\n    height: 100%;\r\n    display:flex;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    overflow-x: auto;\r\n   \r\n}\r\n\r\n.card-list, .card-list-add {\r\n    display: flex;\r\n    flex-direction: column; \r\n    background-color: $cardColor;\r\n    margin: 5px;\r\n    width: 240px;\r\n    border-radius: 3px;\r\n}\r\n\r\n.card-list-add {\r\n    width: 200px;\r\n}\r\n\r\n.card-list-add div input {\r\n    margin: auto;\r\n}\r\n\r\n.card-title h3 {\r\n    font-size: 14px;\r\n    padding: 5px;\r\n    margin-top: 10px;\r\n \r\n}\r\n\r\n.card-title {\r\n    height: 40px;\r\n}\r\n\r\n.card-content-box {\r\n    display: flex;\r\n    flex-direction: column;\r\n    min-height: auto;\r\n    max-height: 450px;\r\n    overflow-y: auto;  \r\n    cursor: move; \r\n}\r\n\r\n.card-content {\r\n    background-color: white;\r\n    margin: 4px;\r\n    border-radius: 2px;\r\n    padding: 5px;\r\n    box-shadow: 1px 0px 2px 0px grey;\r\n    font-size: 14px;\r\n    height:auto;\r\n    width: auto;\r\n    \r\n   \r\n \r\n}\r\n\r\n.card-btn button {\r\n    height:32px;\r\n    width: 100%;\r\n    text-align: left;   \r\n    background-color: $cardColor; \r\n    border: 0;\r\n    padding: 5px;\r\n    border-radius: 3px;\r\n   \r\n}\r\n\r\n\r\n\r\n.card-btn button:hover {\r\n   background-color: $cardColorHover;\r\n   cursor: pointer;\r\n  \r\n}\r\n\r\n.card-btn {\r\n    padding: 5px;\r\n}\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"8538cdd41974b2db7e86477b82321bff","cacheData":{"env":{}}}